{
    "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
    "linterOptions": {
      "exclude": [
        "config/**/*.js",
        "config_data/**/*.js",
        "node_modules/**/*.ts"
      ]
    },
    "rules": {
      "no-bitwise": false,
      "ordered-imports": false,
      "no-console": false,
      "no-reference": false,
      "jsdoc-format": [true, "check-multiline-start"],
      "object-literal-sort-keys": false,
      "prefer-for-of": false,
      "jsx-no-bind": false,
      "no-shadowed-variable": false,
      "no-debugger": false,
      "jsx-no-lambda": false
    }
  }
  